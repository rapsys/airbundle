{% extends '@RapsysAir/mail/body.html.twig' %}
{% block title %}{{ subject }}{% endblock %}
{% block content %}
	<table class="content">
		<tr>
			<td>&nbsp;</td>
			<td>
				<h2>{{ 'Subject: %subject%'|trans({'%subject%': subject}) }}</h2>
				<p>{{ message|nl2br }}</p>
			</td>
			<td>&nbsp;</td>
		</tr>
	</table>
{% endblock %}
